{
  "name": "mern-blog-app",
  "version": "1.0.0",
  "description": "blog app using mongodb, express, react, nodejs",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "install-server": "npm i",
    "start-server": "nodemon server/index.js",
    "install-client": "npm i --prefix client",
    "start-client": "npm run dev --prefix client",
    "dev": "concurrently \"npm run start-server\" \"npm run start-client\""
  },
  "keywords": [],
  "author": "Ali",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.4.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4"
  }
}
